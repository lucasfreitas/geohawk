# Authentication
API_KEY=change_this_to_a_secure_api_key  # Required: API key for endpoint authentication

# Server Configuration
PORT=3000                                # Optional: Server port number (default: 3000)
NODE_ENV=production                      # Optional: Environment mode (production/development)

# MaxMind GeoIP2 Database Paths
GEOIP_CITY_DB=geoip-bases/GeoLite2-City.mmdb       # Path to City database
GEOIP_COUNTRY_DB=geoip-bases/GeoLite2-Country.mmdb # Path to Country database
GEOIP_ASN_DB=geoip-bases/GeoLite2-ASN.mmdb        # Path to ASN database

# Optional Settings
# LOG_LEVEL=info                         # Logging level (debug/info/warn/error)
# RATE_LIMIT_WINDOW=15                   # Rate limiting window in minutes
# RATE_LIMIT_MAX=100                     # Maximum requests per window
# CORS_ORIGIN=*                          # CORS origin configuration
# SWAGGER_PATH=api                       # Custom Swagger UI path
